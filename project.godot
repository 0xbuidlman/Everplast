; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "EnemyComponentManager",
"language": "GDScript",
"path": "res://mobs/components/component_manager.gd"
}, {
"base": "CanvasLayer",
"class": "FadePlayer",
"language": "GDScript",
"path": "res://ui/fade_player/fade_player.gd"
}, {
"base": "Control",
"class": "Main",
"language": "GDScript",
"path": "res://main.gd"
}, {
"base": "Node2D",
"class": "Player",
"language": "GDScript",
"path": "res://player/player.gd"
} ]
_global_script_class_icons={
"EnemyComponentManager": "",
"FadePlayer": "",
"Main": "",
"Player": ""
}

[application]

config/name="Everplast"
run/main_scene="res://main.tscn"
config/use_custom_user_dir=true
boot_splash/image="res://ui/8x8.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/quit_on_go_back=false

[audio]

default_bus_layout="res://audiobuslayout.tres"

[autoload]

UI="*res://autoload/ui.gd"
Globals="*res://autoload/globals.gd"
LevelController="*res://autoload/level_controller.gd"
PlayerStats="*res://autoload/player_stats.gd"
Signals="*res://autoload/signals.gd"
MusicController="*res://autoload/music_controller.gd"
QuickPlay="*res://autoload/quick_play.gd"
FileLocations="*res://autoload/file_locations.gd"

[debug]

gdscript/warnings/unused_signal=false

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/smoothing/plugin.cfg" )

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

